{
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "no-title",
			"postTypes": [
				"page"
			],
			"title": "Page (No Title)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [ "#0C1C31", "#f9f9f9" ],
					"slug": "foreground-and-background",
					"name": "Foreground and background"
				},
				{
					"colors": [ "#f9f9f9", "#0C1C31" ],
					"slug": "background-and-foreground",
					"name": "Background and foreground"
				}
			],
			"gradients": [
				{
					"slug": "vertical-secondary-to-tertiary",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--tertiary) 100%)",
					"name": "Vertical secondary to tertiary"
				},
				{
					"slug": "vertical-secondary-to-background",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical secondary to background"
				},
				{
					"slug": "vertical-tertiary-to-background",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--tertiary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical tertiary to background"
				},
				{
					"slug": "diagonal-primary-to-primary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--primary) 100%)",
					"name": "Diagonal primary to primary"
				},
				{
					"slug": "diagonal-secondary-to-background",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--secondary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal secondary to background"
				},
				{
					"slug": "diagonal-background-to-secondary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal background to secondary"
				},
				{
					"slug": "diagonal-tertiary-to-background",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--tertiary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal tertiary to background"
				},
				{
					"slug": "diagonal-background-to-tertiary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--tertiary) 50%)",
					"name": "Diagonal background to tertiary"
				}
			],
            "palette":  [
				{
					"slug": "foreground",
					"color": "#0C1C31",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#f9f9f9",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#0A9499",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FF7700",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#6E6E7F",
					"name": "Tertiary"
				},
				{
					"slug": "borders",
					"color": "#CACACA",
					"name": "Borders"
				}
			]
		},
		"custom": {
			"spacing": {
				"gap": "1.5rem",
				"outer": "1.5rem"
			},
			"typography": {
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.5,
					"normal": 1.7
				}
			},
            "border-radius": {
                "normal": "4px"
            }
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Source Sans\",sans-serif",
					"name": "Source Sans",
					"slug": "primary"
				},
				{
					"fontFamily": "\"Source Serif\",serif",
					"name": "Source Serif",
					"slug": "secondary"
				},
                {
                    "fontFamily": "Menlo,Consolas,monaco,monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
			],
			"fontSizes": [
                {
                    "name": "Tiny",
                    "size": ".875rem",
                    "slug": "tiny"
                },
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.9rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.75rem, 3vw, 2.5rem)",
					"slug": "x-large"
				},
				{
					"name": "Huge",
					"size": "clamp(2.5rem, 4vw, 3.25rem)",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1200px"
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)",
                    "width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "0.8em",
						"right": "1.2em",
						"bottom": "0.8em",
						"left": "1.2em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"core/categories": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
            "core/code": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)",
					"width": "0px"
                },
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--background)"
                },
                "spacing": {
					"padding": {
						"top": "2rem",
						"right": "2rem",
						"bottom": "2rem",
						"left": "2rem"
					}
				},
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/image": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)"
                }
            },
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				}
			},
			"core/navigation-link": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/navigation-submenu": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
            "core/query-title": {
                "typography": {
                    "fontWeight": "300",
                    "lineHeight": "var(--wp--custom--typography--line-height--small)"
                }
            },
			"core/page-list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
            "core/paragraph": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)"
                }
            },
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)"
				}
			},
			"core/quote": {
				"spacing": {
					"padding": {
						"top": "0.5rem",
						"bottom": "0.5rem",
						"left": "2rem"
					}
				}
			},
            "core/separator": {
                "border": {
                    "width": "0 0 1px 0"
                }
            },
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"core/tag-cloud": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontSize": "calc(0.8 * var(--wp--preset--font-size--large))",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--gap)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
