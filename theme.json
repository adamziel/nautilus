				},
				{
					"slug": "accent",
					"color": "#FF7700",
					"name": "Accent"
				},
				{
					"slug": "info",
					"color": "#027AB6",
					"name": "Info"
				},
				{
					"slug": "warning",
					"color": "#B91C1C",
					"name": "Warning"
				},
				{
