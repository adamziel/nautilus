{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [ "page", "post" ],
			"title": "Blank"
		},
		{
			"name": "page-has-title",
			"postTypes": [ "page" ],
			"title": "Page (Has Title)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [ "#0F172A", "#F9FAFB" ],
					"slug": "primary-and-neutral",
					"name": "Primary and Neutral"
				},
				{
					"colors": [ "#F9FAFB", "#0F172A" ],
					"slug": "neutral-and-primary",
					"name": "Neutral and Primary"
				},
				{
					"colors": [ "#0B8082", "#F9FAFB" ],
					"slug": "secondary-and-neutral",
					"name": "Secondary and Neutral"
				},
				{
					"colors": [ "#F9FAFB", "#0B8082" ],
					"slug": "neutral-and-secondary",
					"name": "Neutral and Secondary"
				},
				{
					"colors": [ "#926D01", "#F9FAFB" ],
					"slug": "tertiary-and-neutral",
					"name": "Tertiary and Neutral"
				},
				{
					"colors": [ "#F9FAFB", "#926D01" ],
					"slug": "neutral-and-tertiary",
					"name": "Neutral and Tertiary"
				}
			],
            "palette":  [
				{
					"slug": "primary",
					"color": "#0F172A",
					"name": "Primary"
				},
				{
					"slug": "primary-light",
					"color": "#475569",
					"name": "Primary (Light)"
				},
				{
					"slug": "primary-lighter",
					"color": "#CBD5E1",
					"name": "Primary (Lighter)"
				},
				{
					"slug": "secondary",
					"color": "#0B8082",
					"name": "Secondary"
				},
				{
					"slug": "secondary-light",
					"color": "#05a9b2",
					"name": "Secondary (Light)"
				},
				{
					"slug": "secondary-lighter",
					"color": "#E0F7F7",
					"name": "Secondary (Lighter)"
				},
				{
					"slug": "tertiary",
					"color": "#926D01",
					"name": "Tertiary"
				},
				{
					"slug": "tertiary-light",
					"color": "#CDAF4B",
					"name": "Tertiary (Light)"
				},
				{
					"slug": "tertiary-lighter",
					"color": "#F5ECCF",
					"name": "Tertiary (Lighter)"
				},
				{
					"slug": "accent",
					"color": "#FF7700",
					"name": "Accent"
				},
				{
					"slug": "info",
					"color": "#027AB6",
					"name": "Info"
				},
				{
					"slug": "warning",
					"color": "#B91C1C",
					"name": "Warning"
				},
				{
					"slug": "neutral",
					"color": "#F9FAFB",
					"name": "Neutral"
				},
				{
					"slug": "neutral-100",
					"color": "#E5E7EB",
					"name": "Neutral (100)"
				},
				{
					"slug": "neutral-300",
					"color": "#D1D5DB",
					"name": "Neutral (300)"
				},
				{
					"slug": "neutral-500",
					"color": "#6B7280",
					"name": "Neutral (500)"
				},
				{
					"slug": "neutral-700",
					"color": "#374151",
					"name": "Neutral (700)"
				},
				{
					"slug": "neutral-900",
					"color": "#111827",
					"name": "Neutral (900)"
				},
				{
					"slug": "white",
					"color": "#FFFFFF",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "background",
					"color": "#000000",
					"name": "Black"
				}
			]
		},
		"custom": {
			"border": {
				"default": "1px solid var(--wp--preset--color--neutral-300)"
			},
            "border-radius": {
                "default": "4px",
				"round": "9999px"
            },
			"box-shadow": {
				"default": "rgba(0, 0, 0, 0.1) 0px 10px 15px -3px, rgba(0, 0, 0, 0.05) 0px 4px 6px -2px;",
				"minimal": "rgba(0, 0, 0, 0.1) 0px 4px 6px -1px, rgba(0, 0, 0, 0.06) 0px 2px 4px -1px;"
			},
			"spacing": {
				"gap": "1.5rem",
				"responsive": "clamp(1.5rem, 2.5vw, 3rem)"
			},
			"typography": {
				"line-height": {
					"x-small": 1,
					"small": 1.2,
					"medium": 1.6,
					"large": 1.75
				}
			}
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "XS",
					"size": "0.5rem",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.5rem, 1.364vw + 1.159rem, 2.25rem)",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "clamp(2.25rem, 1.364vw + 1.909rem, 3rem)",
					"slug": "x-large"
				},
				{
					"name": "2XL",
					"size": "clamp(3rem, 3.603vw + 2.091rem, 5rem)",
					"slug": "xx-large"
				},
				{
					"name": "3XL",
					"size": "clamp(5rem, 3.636vw + 4.091rem, 7rem)",
					"slug": "xxx-large"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Source Sans",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "200 900",
							"src": [ "file:./assets/fonts/source-sans/SourceSans3VF-Roman.ttf.woff2" ]
						},
						{
							"fontFamily": "Source Sans",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "200 900",
							"src": [ "file:./assets/fonts/source-sans/SourceSans3VF-Italic.ttf.woff2" ]
						}
					],
					"fontFamily": "\"Source Sans\",sans-serif",
					"name": "Source Sans",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Source Serif",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "200 900",
							"src": [ "file:./assets/fonts/source-serif/SourceSerif4Variable-Roman.ttf.woff2" ]
						},
						{
							"fontFamily": "Source Serif",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "200 900",
							"src": [ "file:./assets/fonts/source-serif/SourceSerif4Variable-Italic.ttf.woff2" ]
						}
					],
					"fontFamily": "\"Source Serif\",serif",
					"name": "Source Serif",
					"slug": "secondary"
				},
                {
                    "fontFamily": "Menlo,Consolas,monaco,monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
			],
			"fontSizes": [
                {
					"fluid": {
						"min": "0.8rem",
						"max": "0.875rem"
					},
                    "name": "Extra Small",
                    "size": "0.875rem",
                    "slug": "x-small"
                },
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.625rem",
						"max": "1.875rem"
					},
					"name": "Extra Large",
					"size": "1.875rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"name": "2X Large",
					"size": "2.5rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2.75rem",
						"max": "3.5rem"
					},
					"name": "3X Large",
					"size": "3.5rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"min": "3.75rem",
						"max": "5rem"
					},
					"name": "4X Large",
					"size": "5rem",
					"slug": "xxxx-large"
				},
				{
					"fluid": {
						"min": "4.75rem",
						"max": "7.125rem"
					},
					"name": "5X Large",
					"size": "7.125rem",
					"slug": "xxxxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "720px",
			"wideSize": "1200px"
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontSize": "var:preset|font-size|x-small",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var:preset|font-family|primary",
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|primary"
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/button": {
				"border": {
                    "width": "1px !important"
				},
				"spacing": {
					"padding": {
						"top": "0.85em !important",
						"right": "1.2em !important",
						"bottom": "0.8em !important",
						"left": "1.2em !important"
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var:preset|spacing|small"
				}
			},
			"core/categories": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontSize": "var:preset|font-size|x-small",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var:preset|font-family|primary",
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|primary"
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
            "core/code": {
                "border": {
                	"radius": "var:custom|border-radius|default",
					"width": "0px"
                },
                "color": {
                	"background": "var:preset|color|primary",
					"text": "var:preset|color|primary-lighter"
                },
                "spacing": {
					"padding": {
						"top": "var:preset|spacing|medium",
						"right": "var:preset|spacing|medium",
						"bottom": "var:preset|spacing|medium",
						"left": "var:preset|spacing|medium"
					}
				},
                "typography": {
                    "fontFamily": "var:preset|font-family|monospace",
                    "fontSize": "var:preset|font-size|small"
                }
            },
            "core/image": {
                "border": {
                    "radius": "var:custom|border-radius|default"
                },
				"elements": {
					"caption": {
						"border": {
							"radius": "var:custom|border-radius|default"
						},
						"spacing": {
							"margin": {
								"top": ".875rem"
							}
						}
					}
				}
            },
			"core/list": {
				"typography": {
					"fontFamily": "var:preset|font-family|secondary"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var:preset|font-family|primary",
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
            "core/query-title": {
                "typography": {
                    "lineHeight": "var:custom|typography|line-height|small"
                }
            },
			"core/page-list": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary"
				}
			},
            "core/paragraph": {
                "typography": {
                    "fontFamily": "var:preset|font-family|secondary",
					"letterSpacing": "-0.1px"
                }
            },
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var:custom|spacing|small"
					}
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var:preset|font-family|primary"
						}
					}
				}
			},
			"core/post-template": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|secondary"
							}
						}
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var:custom|border-radius|default"
				}
			},
			"core/pullquote": {
				"border": {
					"radius": "var:custom|border-radius|default",
					"width": "0px"
				},
				"color": {
					"background": "var:preset|color|neutral-100"
				},
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
            "core/separator": {
                "border": {
                    "width": "0 0 1px 0"
                }
            },
			"core/site-title": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/tag-cloud": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var:preset|color|neutral",
			"text": "var:preset|color|primary"
		},
		"elements": {
			"button": {
				"border": {
					"color": "transparent",
					"radius": "var(--wp--custom--border-radius--default)",
					"style": "solid",
                    "width": "1px"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|neutral"
				},
				"spacing": {
					"padding": {
						"top": "0.85em",
						"right": "1.2em",
						"bottom": "0.8em",
						"left": "1.2em"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"lineHeight": "1",
					"textTransform": "uppercase"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|primary-light",
						"text": "var:preset|color|neutral"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var:preset|color|primary-light"
				},
				"spacing": {
					"margin": {
						"bottom": "0rem !important"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:custom|typography|line-height|medium"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var:custom|typography|line-height|small"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xxx-large"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large"
				}
			},
			"h4": {
				"color": {
					"text": "var:preset|color|tertiary"
				},
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"color": {
					"text": "var:preset|color|primary-light"
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|secondary"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "currentColor"
					}
				},
				":focus": {
					"color": {
						"text": "currentColor"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"right": "var:preset|spacing|large",
				"left": "var:preset|spacing|large"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|primary",
			"fontSize": "var:preset|font-size|medium",
			"lineHeight": "var:custom|typography|line-height|medium"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
