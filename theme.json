{
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "page-home",
			"title": "Homepage",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "single-no-separators",
			"title": "Single Post (No Separators)",
			"postTypes": [
				"post"
			]
		},
		{
			"name": "page-no-separators",
			"title": "Page (No Separators)",
			"postTypes": [
				"page"
			]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [ "#0C1C31", "#f9f9f9" ],
					"slug": "foreground-and-background",
					"name": "Foreground and background"
				},
				{
					"colors": [ "#f9f9f9", "#0C1C31" ],
					"slug": "background-and-foreground",
					"name": "Background and foreground"
				}
			],
			"gradients": [
				{
					"slug": "vertical-secondary-to-tertiary",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--tertiary) 100%)",
					"name": "Vertical secondary to tertiary"
				},
				{
					"slug": "vertical-secondary-to-background",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical secondary to background"
				},
				{
					"slug": "vertical-tertiary-to-background",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--tertiary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical tertiary to background"
				},
				{
					"slug": "diagonal-primary-to-primary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--primary) 100%)",
					"name": "Diagonal primary to primary"
				},
				{
					"slug": "diagonal-secondary-to-background",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--secondary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal secondary to background"
				},
				{
					"slug": "diagonal-background-to-secondary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal background to secondary"
				},
				{
					"slug": "diagonal-tertiary-to-background",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--tertiary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal tertiary to background"
				},
				{
					"slug": "diagonal-background-to-tertiary",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--tertiary) 50%)",
					"name": "Diagonal background to tertiary"
				}
			],
            "palette":  [
					{
						"slug": "foreground",
						"color": "#0C1C31",
						"name": "Foreground"
					},
    				{
    					"slug": "background",
    					"color": "#f9f9f9",
    					"name": "Background"
    				},
    				{
    					"slug": "primary",
    					"color": "#0A9499",
    					"name": "Primary"
    				},
    				{
    					"slug": "secondary",
    					"color": "#FF7700",
    					"name": "Secondary"
    				},
    				{
    					"slug": "tertiary",
    					"color": "#6E6E7F",
    					"name": "Tertiary"
    				},
					{
						"slug": "borders",
						"color": "#CACACA",
						"name": "Borders"
					}
    			]
		},
		"custom": {
			"spacing": {
				"outer": "1.5rem"
			},
			"typography": {
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.7
				}
			},
            "border-radius": {
                "normal": "4px"
            }
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Source Sans\",sans-serif",
					"name": "Source Sans",
					"slug": "source-sans"
				},
				{
					"fontFamily": "\"Source Serif\",serif",
					"name": "Source Serif",
					"slug": "source-serif"
				},
                {
                    "fontFamily": "Menlo,Consolas,monaco,monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
			],
			"fontSizes": [
                {
                    "name": "Tiny",
                    "size": ".875rem",
                    "slug": "tiny"
                },
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large"
				},
				{
					"name": "Huge",
					"size": "clamp(2.5rem, 4vw, 3rem)",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1120px"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)",
                    "width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "1em",
						"right": "1.5em",
						"bottom": "1em",
						"left": "1.5em"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": 500,
					"letterSpacing": "1px",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
            "core/code": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)"
                },
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--background)"
                },
                "spacing": {
					"padding": {
						"top": "30px",
						"right": "30px",
						"bottom": "30px",
						"left": "30px"
					}
				},
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/image": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)"
                }
            },
            "core/query-title": {
                "typography": {
                    "fontWeight": "300",
                    "lineHeight": "var(--wp--custom--typography--line-height--small)"
                }
            },
            "core/quote": {
                "border": {
                    "width": "0 0 0 1px"
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--source-serif)"
                }
            },
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)"
				}
			},
            "core/post-terms": {
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--tertiary)"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--tiny)"
                }
            },
            "core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"style": "solid",
					"width": "1px"
				},
				"spacing": {
					"margin": {
						"bottom": "30px"
					},
					"padding": {
						"top": "30px",
						"right": "30px",
						"bottom": "30px",
						"left": "30px"
					}
				}
			},
            "core/quote": {
				"spacing": {
					"margin": {
						"top": "0px",
						"right": "0px",
						"bottom": "30px",
						"left": "0px"
					},
                    "padding": {
						"top": "30px",
						"right": "30px",
						"bottom": "30px",
						"left": "30px"
					}
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--smaller)"
				}
			},
            "core/separator": {
                "border": {
                    "width": "0 0 1px 0"
                }
            },
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": 500,
					"letterSpacing": "1px",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "52px"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "32px",
					"fontWeight": "600",
					"lineHeight": "40px"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h4": {
				"typography": {
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontWeight": "700",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "400",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--source-sans)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
