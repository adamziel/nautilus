{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "page-has-title",
			"postTypes": [
				"page"
			],
			"title": "Page (Has Title)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#0F172A",
						"#F9FAFB"
					],
					"name": "Primary and Neutral",
					"slug": "primary-and-neutral"
				},
				{
					"colors": [
						"#F9FAFB",
						"#0F172A"
					],
					"name": "Neutral and Primary",
					"slug": "neutral-and-primary"
				},
				{
					"colors": [
						"#0B8082",
						"#F9FAFB"
					],
					"name": "Secondary and Neutral",
					"slug": "secondary-and-neutral"
				},
				{
					"colors": [
						"#F9FAFB",
						"#0B8082"
					],
					"name": "Neutral and Secondary",
					"slug": "neutral-and-secondary"
				},
				{
					"colors": [
						"#926D01",
						"#F9FAFB"
					],
					"name": "Tertiary and Neutral",
					"slug": "tertiary-and-neutral"
				},
				{
					"colors": [
						"#F9FAFB",
						"#926D01"
					],
					"name": "Neutral and Tertiary",
					"slug": "neutral-and-tertiary"
				}
			],
			"palette": [
				{
					"color": "#0F172A",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#475569",
					"name": "Primary (Light)",
					"slug": "primary-light"
				},
				{
					"color": "#0B8082",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#05a9b2",
					"name": "Secondary (Light)",
					"slug": "secondary-light"
				},
				{
					"color": "#926D01",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#CDAF4B",
					"name": "Tertiary (Light)",
					"slug": "tertiary-light"
				},
				{
					"color": "#F9FAFB",
					"name": "Neutral",
					"slug": "neutral"
				},
				{
					"color": "#E5E7EB",
					"name": "Neutral (100)",
					"slug": "neutral-100"
				},
				{
					"color": "#D1D5DB",
					"name": "Neutral (300)",
					"slug": "neutral-300"
				},
				{
					"color": "#6B7280",
					"name": "Neutral (500)",
					"slug": "neutral-500"
				},
				{
					"color": "#374151",
					"name": "Neutral (700)",
					"slug": "neutral-700"
				},
				{
					"color": "#111827",
					"name": "Neutral (900)",
					"slug": "neutral-900"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#FF7700",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#027AB6",
					"name": "Info",
					"slug": "info"
				},
				{
					"color": "#B91C1C",
					"name": "Warning",
					"slug": "warning"
				}
			]
		},
		"custom": {
			"border": {
				"default": "1px solid var(--wp--preset--color--neutral-300)"
			},
			"border-radius": {
				"default": "4px",
				"round": "9999px"
			},
			"box-shadow": {
				"default": "rgba(0, 0, 0, 0.1) 0px 10px 15px -3px, rgba(0, 0, 0, 0.05) 0px 4px 6px -2px;",
				"minimal": "rgba(0, 0, 0, 0.1) 0px 4px 6px -1px, rgba(0, 0, 0, 0.06) 0px 2px 4px -1px;"
			},
			"typography": {
				"line-height": {
					"large": 1.75,
					"medium": 1.6,
					"small": 1.2,
					"x-small": 1
				}
			}
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1200px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "XS",
					"size": "0.5rem",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.5rem, 1.364vw + 1.159rem, 2.25rem)",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "clamp(2.25rem, 1.364vw + 1.909rem, 3rem)",
					"slug": "x-large"
				},
				{
					"name": "2XL",
					"size": "clamp(3rem, 3.603vw + 2.091rem, 5rem)",
					"slug": "xx-large"
				},
				{
					"name": "3XL",
					"size": "clamp(5rem, 3.636vw + 4.091rem, 7rem)",
					"slug": "xxx-large"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/inter/Inter.var.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\",sans-serif",
					"name": "Inter",
					"slug": "primaryw"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/source-sans/SourceSans3VF-Roman.ttf.woff2"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/source-sans/SourceSans3VF-Italic.ttf.woff2"
							]
						}
					],
					"fontFamily": "\"Source Sans\",sans-serif",
					"name": "Source Sans",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Source Serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/source-serif/SourceSerif4Variable-Roman.ttf.woff2"
							]
						},
						{
							"fontFamily": "Source Serif",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/source-serif/SourceSerif4Variable-Italic.ttf.woff2"
							]
						}
					],
					"fontFamily": "\"Source Serif\",serif",
					"name": "Source Serif",
					"slug": "secondary"
				},
				{
					"fontFamily": "Menlo,Consolas,monaco,monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.8rem"
					},
					"name": "Extra Small",
					"size": "0.875rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1.125rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.375rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.875rem",
						"min": "1.625rem"
					},
					"name": "Extra Large",
					"size": "1.875rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "2X Large",
					"size": "2.5rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "3.5rem",
						"min": "2.75rem"
					},
					"name": "3X Large",
					"size": "3.5rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"max": "5rem",
						"min": "3.75rem"
					},
					"name": "4X Large",
					"size": "5rem",
					"slug": "xxxx-large"
				},
				{
					"fluid": {
						"max": "7.125rem",
						"min": "4.25rem"
					},
					"name": "5X Large",
					"size": "7.125rem",
					"slug": "xxxxx-large"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Small",
					"slug": "small",
					"shadow": "rgba(0, 0, 0, 0.1) 0px 4px 6px -1px, rgba(0, 0, 0, 0.06) 0px 2px 4px -1px"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"shadow": "rgba(0, 0, 0, 0.1) 0px 10px 15px -3px, rgba(0, 0, 0, 0.05) 0px 4px 6px -2px"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)",
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/categories": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)",
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"core/code": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary) !important",
					"text": "var(--wp--preset--color--neutral)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)"
				},
				"elements": {
					"caption": {
						"border": {
							"radius": "var(--wp--custom--border-radius--default)"
						},
						"spacing": {
							"margin": {
								"top": "1rem"
							}
						}
					}
				},
				"css": "&>figcaption { text-align: center }"
			},
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)",
							"textDecoration": "none"
						}
					}
				}
			},
			"core/page-list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"letterSpacing": "-0.1px"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)"
				}
			},
			"core/post-template": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							}
						},
						"color": {
							"text": "currentColor"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--neutral-100)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/query-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"core/separator": {
				"border": {
					"width": "0 0 1px 0"
				}
			},
			"core/search": {
				"elements": {
					"button": {
						"border": {
							"radius": "var(--wp--custom--border-radius--default) !important"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "currentColor"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/tag-cloud": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-small) !important",
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "color-mix(in srgb, var(--wp--preset--color--primary) 90%, var(--wp--preset--color--neutral) 10%)"
					}
				},
				"border": {
					"color": "transparent",
					"radius": "var(--wp--custom--border-radius--default)",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--neutral)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.8em",
						"left": "1.2em",
						"right": "1.2em",
						"top": "0.8em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--x-small)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary-light)"
				},
				"spacing": {
					"margin": {
						"bottom": "0rem !important"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--primary-light)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "currentColor"
					}
				},
				":hover": {
					"color": {
						"text": "currentColor"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"left": "var(--wp--preset--spacing--large)",
				"right": "var(--wp--preset--spacing--large)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}