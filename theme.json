				{
					"slug": "neutral-900",
					"color": "#111827",
					"name": "Neutral (900)"
				}
			]
		},
		"custom": {
			"spacing": {
				"gap": "1.5rem",
				"outer": "1.5rem"
			},
			"typography": {
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.5,
					"normal": 1.7
				}
			},
            "border-radius": {
                "normal": "4px",
				"round": "9999px"
            }
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Source Sans\",sans-serif",
					"name": "Source Sans",
					"slug": "primary"
				},
				{
					"fontFamily": "\"Source Serif\",serif",
					"name": "Source Serif",
					"slug": "secondary"
				},
                {
                    "fontFamily": "Menlo,Consolas,monaco,monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
			],
			"fontSizes": [
                {
                    "name": "Tiny",
                    "size": ".875rem",
                    "slug": "tiny"
                },
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.9rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.75rem, 3vw, 2.5rem)",
					"slug": "x-large"
				},
				{
					"name": "Huge",
					"size": "clamp(2.5rem, 4vw, 3.25rem)",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1200px"
		},
		"blocks": {
			"core/code": {
				"border": {
					"color": false,
					"radius": false,
					"style": false,
					"width": false
				},
				"color": {
					"custom": false,
					"customGradient": false,
					"gradients": [],
					"palette": []
				},
				"spacing": {
					"padding": false
				},
				"typography": {
					"customFontSize": false,
		            "fontFamilies": [],
		            "fontSizes": [],
		            "fontStyle": false,
		            "fontWeight": false,
		            "letterSpacing": false,
					"lineHeight": false,
		            "textDecoration": false,
		            "textTransform": false
				}
			},
			"core/button": {
				"color": {
					"custom": false,
					"customGradient": false,
					"gradients": [],
					"palette":  [
						{
							"slug": "primary",
							"color": "#0F172A",
							"name": "Primary"
						},
						{
							"slug": "secondary",
							"color": "#0B8082",
							"name": "Secondary"
						},
						{
							"slug": "tertiary",
							"color": "#926D01",
							"name": "Tertiary"
						},
						{
							"slug": "accent",
							"color": "#FF7700",
							"name": "Accent"
						},
						{
							"slug": "info",
							"color": "#027AB6",
							"name": "Info"
						},
						{
							"slug": "warning",
							"color": "#B91C1C",
							"name": "Warning"
						},
						{
							"slug": "neutral",
							"color": "#F9FAFB",
							"name": "Neutral"
						}
					]
				},
				"border": {
					"color": false,
					"radius": false,
					"style": false,
					"width": false
				},
				"typography": {
					"fontFamilies": []
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"core/button": {
				"border": {
					"color": "transparent",
					"radius": "var(--wp--custom--border-radius--normal)",
					"style": "solid",
                    "width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--neutral)"
				},
				"spacing": {
					"padding": {
						"top": "0.8em",
						"right": "1.2em",
						"bottom": "0.8em",
						"left": "1.2em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"core/categories": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
            "core/code": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)",
					"width": "0px"
                },
                "color": {
                    "background": "var(--wp--preset--color--neutral-100)",
                    "text": "var(--wp--preset--color--primary)"
                },
                "spacing": {
					"margin": {
						"top": "calc(1.5 * var(--wp--custom--spacing--gap))",
						"bottom": "calc(1.5 * var(--wp--custom--spacing--gap))"
					},
					"padding": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "var(--wp--custom--spacing--gap)"
					}
				},
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/image": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--normal)"
                }
            },
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				}
			},
			"core/navigation-link": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/navigation-submenu": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
            "core/query-title": {
                "typography": {
                    "fontWeight": "300",
                    "lineHeight": "var(--wp--custom--typography--line-height--small)"
                }
            },
			"core/pullquote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--neutral-100)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/page-list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
            "core/paragraph": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)"
                }
            },
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--normal)"
				}
			},
			"core/quote": {
				"spacing": {
					"padding": {
						"top": "0.5rem",
						"bottom": "0.5rem",
						"left": "2rem"
					}
				}
			},
            "core/separator": {
                "border": {
                    "width": "0 0 1px 0"
                }
            },
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			},
			"core/tag-cloud": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": 500,
					"letterSpacing": "0.5px",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral-light)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontSize": "calc(0.8 * var(--wp--preset--font-size--large))",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"letterSpacing": "0.5px",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--gap)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
